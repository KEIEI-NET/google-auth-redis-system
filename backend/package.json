{
  "name": "google-auth-employee-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:setup": "prisma generate && prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node src/database/seeds/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/cookie-parser": "^1.4.9",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.17.0",
    "express-validator": "^7.0.0",
    "google-auth-library": "^9.4.0",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "@types/validator": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.6.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
